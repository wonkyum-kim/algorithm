/* Insertion sort C++ */

#include <vector>
#include <iostream>

using namespace std;

template <typename T>
void insertion_sort(std::vector<T>& vec, const int& left, const int& right) 
{
	for (int i = left; i < right; ++i) 
    {
		for (int j = i + 1; j > left && vec[j] < vec[j - 1]; --j)
			std::swap(vec[j], vec[j - 1]);
	}
}


template<typename T>
void print_sort(vector<T>& vec, const int& n)
{
	for (int i = 0; i < n; ++i)
		cout << vec[i] << " ";
	cout << endl;
}

int main()
{
	vector<int> vec{ 5,2,4,6,1,3 };
	int n = vec.size();
	insertion_sort(vec, 0, n-1);
	print_sort(vec, n);
	return 0;
}
